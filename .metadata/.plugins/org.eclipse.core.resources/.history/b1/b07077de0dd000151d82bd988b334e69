package nrnoble;

public class Vars
{
	private int a = 0;
	private int b = 0;
	private int c = 0;
	private int d = 0;
	private int r1 = 0;
	private int r2 = 0;
	private String callingMethod = "";

	public Vars(int a, int b, int c, int d, int r1, int r2)
	{
		super();
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
		this.r1 = r1;
		this.r2 = r2;
	}

	public Vars()
	{

		
	}

	@Override
	public String toString()
	{
		return "Vars []";
	}

}
