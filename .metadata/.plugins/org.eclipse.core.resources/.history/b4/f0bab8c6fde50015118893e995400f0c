package nrnoble;

import java.util.ArrayList;
import java.util.List;

public class PrimesData 
{
	
	private static List<Integer> primesList = new ArrayList<Integer>();
	private static List<Integer> primesList_2 = new ArrayList<Integer>();
	
	public PrimesData()
	{
		// TODO Auto-generated constructor stub
	}
	
	public synchronized static void add(int _prime)
	{
		primesList.add(_prime);
		System.out.print(".");
	}
	
	
	public synchronized static List<Integer> getPrimes()
	{
		return primesList;
	}
	
	

}
